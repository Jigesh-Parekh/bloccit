 <h1><%= @question.title %></h1>
 <h2><%= @question.body %></h2>
 <h2><%= @question.resolved %></h2>

<div class = "row">
	<div class = "col-md-8">
		<p><%= @question.body %> </p>
	</div>

	<div class = "col-md-4">
		<%= link_to "Edit", edit_question_path(@question), class: 'btn btn-success' %>

		<%= link_to "Delete Question", @question, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to delete this?'} %>
	</div>
</div>
